{
	"name": "toptop_vag",
	"version": "0.0.1",
	"description": "top-repo that points to the other toptop repositories",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg/toptop_vag.git"
	},
	"homepage": "https://github.com/charlyoleg/toptop_vag#readme",
	"author": "charlyoleg",
	"license": "ISC",
	"keywords": [
		"toptop",
		"vag"
	],
	"type": "module",
	"main": "index.js",
	"scripts": {
		"vag-list": "vag --importYaml=toptop_repos.yml list",
		"vag-clone": "vag --importYaml=toptop_repos.yml clone",
		"vag-status": "vag --importYaml=toptop_repos.yml --only_configured status",
		"vag-diff": "vag --importYaml=toptop_repos.yml --only_configured diff",
		"vag-branch": "vag branch",
		"vag-remote": "vag remote",
		"vag-pull": "vag pull",
		"vag-push": "vag push",
		"vag-verify": "vag verify --importYaml=toptop_repos.yml",
		"vag-versions": "vag versions",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"vag_tools": "^0.0.7"
	}
}
